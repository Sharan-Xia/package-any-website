{
    "name": "package-any-website",
    "version": "1.0.0",
    "description": "使用 Electron 将任何网站为桌面端应用",
    "main": "config.js",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "pack:mac": "electron-builder --mac",
        "pack:win": "electron-builder --win",
        "pack:linux": "electron-builder --linux",
        "pack:all": "electron-builder -mwl",
        "pack": "node index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rmlzy/package-any-website.git"
    },
    "author": "rmlzy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rmlzy/package-any-website/issues"
    },
    "homepage": "https://github.com/rmlzy/package-any-website#readme",
    "devDependencies": {
        "electron": "^4.0.5",
        "electron-builder": "^20.38.5",
        "enquirer": "^2.3.0",
        "shelljs": "^0.8.3"
    },
    "build": {
        "appId": "com.example.electron-cra",
        "productName": "南极智云",
        "asar": true,
        "extends": null,
        "directories": {
            "buildResources": "dist"
        },
        "mac": {
            "icon": "images/app-icon.png"
        },
        "win": {
            "icon": "images/app-icon.ico"
        }
    },
    "dependencies": {}
}
